{
  "name": "demo",
  "version": "0.0.0",
  "private": true,
  "main": "server.js",
  "scripts": {
    "test": "jest --coverage",
    "start": "node server.js"
  },
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.4"
  },
  "dependencies": {
    "express": "^4.21.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "server.js",
      "CICD-testing/**/*.js",
      "!**/node_modules/**",
      "!**/tests/**"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
